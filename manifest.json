{
    "name": "BoomConsole",
    "description": "Conceptualizes browsers for our clients to the freeSchema.com,inc database.",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "background": {
        "service_worker": "./service-worker.js",
        "type": "module"
    },
    "action": {
        "default_icon": {
            "16": "./icons/16.png",
            "32": "./icons/32.png"
        },
        "default_title": "Boom Console"
    },
    "permissions": [
        "scripting",
        "activeTab",
        "storage",
        "desktopCapture",
        "tabs"
    ],
    "options_page": "./options/options.html",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "./scripts/content-script.js"
            ]
        }
    ],
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+B",
                "mac": "Command+B"
            }
        }
    }
}